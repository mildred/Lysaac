Section Header

  + name := TYPE_LINT;

Section Inherit

  + parent_type :Expanded TYPE;
  
Section Public
  
  //////////////////////////////////////////////////////////////////////////////
  //
  // Identification
  //

  - name     :CSTRING <- "Integer";
  - expanded :BOOLEAN <- TRUE;
  - block    :BOOLEAN <- FALSE;
  - void     :BOOLEAN <- FALSE;
  
  //////////////////////////////////////////////////////////////////////////////
  //
  // Create Register
  //

  - can_create_reg :BOOLEAN <- TRUE;

  - create_reg :REG <-
  (
    REG.clone.make_type (LOW_LEVEL_TYPE.int)
  );
  
  //////////////////////////////////////////////////////////////////////////////
  //
  // Implementation Slots
  //
  
  + to_impl :TYPE_IMPL;
  
  //////////////////////////////////////////////////////////////////////////////
  //
  // Other
  //
  
  - keywords_str :CSTRING <- "";
  
  //////////////////////////////////////////////////////////////////////////////
  //
  // Creation
  //
  
  - make_with_implementation impl:TYPE_IMPL :SELF <-
  (
    to_impl := impl;
    Self
  );

