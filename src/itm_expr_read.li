Section Header

  + name := ITM_EXPR_READ;

Section Inherit

  + parent_itm_expr :Expanded ITM_EXPR;

Section Public

  - is_void :BOOLEAN <- FALSE;

  + slot     :CSTRING;
  + argument :ITM_EXPR;

  - compile_in list:LIST <-
  (
    list.add_instruction (INSTR_CALL.clone.make slot);
  );

Section PARSER
  
  - make (s:CSTRING, arg:ITM_EXPR) :SELF <-
  (
    slot := s;
    argument := arg;
    Self
  );

