Section Header

  + name := ITM_EXPR_STR;

Section Inherit

  + parent_itm_expr :Expanded ITM_EXPR;

Section Public

  - is_void :BOOLEAN <- FALSE;

  + string :CSTRING;

  - compile_in list:LIST :SELF <-
  (
    cluster_parent.type_for_role "String".if_false { e:ERR;
      results <! e.extend (position, "Discarding string");
    } else { t:TYPE;
      results << EXPR.clone.make_reg
        (INSTR_STRING.clone.make string static_type t.add_in list.result);
    };
    Self
  );
  
Section PARSER
  
  - make s:CSTRING :SELF <-
  (
    results := EXPR1.clone;
    string  := s;
    Self
  );

