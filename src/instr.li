Section Header

  + name := INSTR;

Section Inherit

  + parent_any :Expanded ANY;

Section Public

  + result :REG := REG;
  
  + list_parent :LIST;
  
  - link <- deferred;

  - generate gen:GENERATOR <- deferred;
  
  + error :ERR := ERR;

  - add_in list:LIST :SELF <-
  [
    -? { list != NULL };
    -? { list_parent = NULL };
  ]
  ( 
    list_parent := list;
    list.add_instruction Self;
    Self
  );


