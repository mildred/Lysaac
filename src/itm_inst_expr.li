Section Header

  + name := ITM_INST_EXPR;

Section Inherit

  + parent_itm_inst :Expanded ITM_INST;

Section Public

  + expression :ITM_EXPR;
  
  - compile_in list:LIST :VAR <- expression.compile_in list;

Section PARSER
  
  - make e:ITM_EXPR :SELF <-
  (
    e.set_itm_parent Self;
    expression := e;
    Self
  );
