Section Header

  + name := ITM_INST_EXPR;

Section Inherit

  + parent_itm_inst :Expanded ITM_INST;

Section Public

  + expression :ITM_EXPR;
  
  - compile_in list:LIST <- (expression.compile_in list;);
  
  - to_string :STRING <-
  ( type_name + "<" + expression.to_string + ">" );
  
  - err :ERR <- expression.err;

Section PARSER
  
  - make e:ITM_EXPR :SELF <-
  (
    e.set_itm_parent Self;
    expression := e;
    Self
  );
