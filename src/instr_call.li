Section Header

  + name := INSTR_CALL;

Section Inherit

  + parent_instr :Expanded INSTR;

Section Public

  - name :CSTRING <- ast.slot;

  - generate gen:GENERATOR <-
  (
    gen.call name args {
      gen.call_arg arg;
    };
  );

Section ITM_EXPR_READ

  - make_itm i:ITM_EXPR_READ argument v:VAR :SELF <-
  (
    arg := v;
    ast := i;
    Self
  );

Section Private

  + ast :ITM_EXPR_READ;
  + arg :VAR;
