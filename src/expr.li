Section Header

  + name := EXPR;
  
  - export := REG, TYPE;

Section Inherit

  + parent_any :Expanded ANY;

Section Public

  + reg :REG
  
  + type :TYPE;

  - make (r:REG, t:TYPE) :SELF <-
  [
    -? { reg.is_valid };
    -? { t != NULL };
    -? { t.base_type = r.base_type };
  ]
  (
    reg  := r;
    type := t;
    Self
  );

  - to_reg  :REG  <- reg;
  - to_type :TYPE <- type;
