Section Header

  + name := ITM_KEYWORDS;

Section Inherit

  + parent_any :Expanded ANY;

Section Public

  - has k:CSTRING :BOOLEAN <-
  (
    keywords.valid_index (keywords.first_index_of k)
  );

Section PARSER

  - make :SELF <-
  (
    keywords := FAST_ARRAY(CSTRING).create 0;
    Self
  );
  
  - add_keyword k:CSTRING <-
  (
    keywords.add_last k;
  );

Section Private

  + keywords :FAST_ARRAY(CSTRING);
  

