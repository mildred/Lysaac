Section Header

  + name := ITM;

Section Inherit

  + parent_any :Expanded ANY;

Section Public

  + itm_parent :ITM;
  
  - cluster_parent   :CLUSTER   <- prototype_parent.cluster;
  - prototype_parent :PROTOTYPE <- itm_parent.prototype_parent;
  - slot_parent      :ITM_SLOT  <- itm_parent.slot_parent;

Section ITM

  - set_itm_parent i:ITM :SELF <-
  (
    itm_parent := i;
    Self
  );
  

