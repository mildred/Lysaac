Section Header

  + name := ITM_EXPR_PROTO;

Section Inherit

  + parent_itm_expr :Expanded ITM_EXPR;

Section Public

  - is_void :BOOLEAN <- FALSE;

  + prototype :CSTRING;
  
  - to_type :TYPE <-
  ( + res :TYPE;
    res := slot_parent.find_type prototype;
    (res = NULL).if {
      warnln ("Could not find type " + prototype);
    };
    res
  );

Section PARSER
  
  - make p:CSTRING :SELF <-
  (
    prototype := p;
    Self
  );

