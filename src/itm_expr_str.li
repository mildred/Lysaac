Section Header

  + name := ITM_EXPR_STR;

Section Inherit

  + parent_itm_expr :Expanded ITM_EXPR;

Section Public

  - is_void :BOOLEAN <- FALSE;

  + string :CSTRING;
  
  - has_results :BOOLEAN <- TRUE;

  + result :EXPR := EXPR;

  - compile_in list:LIST :SELF <-
  (
    cluster_parent.type_for_role "String".if { t:TYPE;
      result := EXPR.clone.make
        (INSTR_STRING.clone.make string static_type t.add_in list.result, t);
    } else {
      err := error(err, position, "Could not find role for String");
    };
    Self
  );
  
  + err :ERR := ERR;
  
Section PARSER
  
  - make s:CSTRING :SELF <-
  (
    err    := ERR;
    string := s;
    Self
  );

