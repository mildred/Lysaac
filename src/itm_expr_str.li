Section Header

  + name := ITM_EXPR_STR;

Section Inherit

  + parent_itm_expr :Expanded ITM_EXPR;

Section Public

  - is_void :BOOLEAN <- FALSE;

  + string :CSTRING;

  - compile_in list:LIST :VAR <-
  (
    INSTR_STRING.clone.make string.add_in list.result
  );
  
Section PARSER
  
  - make s:CSTRING :SELF <-
  (
    string := s;
    Self
  );

