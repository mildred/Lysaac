Section Header

  + name := ITM_EXPR_SYM;

Section Inherit

  + parent_itm_expr :Expanded ITM_EXPR;

Section Public

  - is_void :BOOLEAN <- FALSE;

  + symbol :CSTRING;

  - compile_in list:LIST :SELF <-
  (
    list.error_collector.add_error(position.error("Unexpected symbol"));
    Self
  );

Section PARSER
  
  - make s:CSTRING :SELF <-
  (
    symbol := s;
    Self
  );

