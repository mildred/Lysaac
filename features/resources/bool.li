Section Header

  + name := Expanded BOOL;

Section Public //BOOL

  + bit_value :BIT <- Dispatch 0;
  
  - dispatcher <- Dispatcher;

Section Public

  - if_true  blc:{} :BOOL <- Deferred;
  - if_false blc:{} :BOOL <- Deferred;

  - if   blc:{} :BOOL <- if_true  blc;
  - else blc:{} :BOOL <- if_false blc;

